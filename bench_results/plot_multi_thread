set terminal postscript eps enhanced color 
filename = 'degree_16_grow_threads_1005.txt'
set output "degree_16_grow_threads.eps" 

# set font "32"

# set font "32"

set key right
set grid
set logscale y
set logscale x

set title font ",10" 
set key title "Multi-threading performance" 
set xlabel "\#threads"
set ylabel 'log time (us)'
set xtics (0,1,2,4,8,16,32)
plot filename using 1:2 w lp t "2^{10} constraints",\
     filename using 1:3 w lp t "2^{15} constraints",\
     filename using 1:4 w lp t "2^{18} constraints",\
     filename using 1:5 w lp t "2^{20} constraints",\


reset
